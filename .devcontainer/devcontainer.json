{
    "name": "rust-remote-container",
    "dockerComposeFile": ["../docker-compose.yml", "docker-compose.extend.yml"],
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "remoteUser": "vscode",
    "service": "rust",
    "workspaceFolder": "/usr/src/app",
    "extensions": [
		"rust-lang.rust",
		"bungcip.better-toml"
	]
}